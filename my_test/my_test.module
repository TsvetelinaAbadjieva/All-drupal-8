<?php

/**
 * @file
 * Contains my_test.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function my_test_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the my_test module.
    case 'help.page.my_test':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My First autogenerated module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function my_test_theme() {
  return [
    'my_test' => [
      'render element' => 'children',
    ],
  ];
}

/**
* @file
*implements the hook_menu() method
*/

function form_module_menu(){
	$items = [];
	$items['/admin/config/system/form_module'] =[
		'title' => 'Custom form',
		'description'=> 'Custom form configutation',
		'route_name' => 'form_module.settings'
	];
	return $items;
}

